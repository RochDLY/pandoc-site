  langid = {french},
  keywords = {autorite,editorialisation,espace numerique,organisation des contenus,philosophie,philosophie du numerique,sciences de l'information et de la communication,theorie des humanites numeriques},
  file = {/home/user/Zotero/storage/MXRJXZCJ/Vitali-Rosati - 2020 - Pour une théorie de l’éditorialisation.pdf}
}

